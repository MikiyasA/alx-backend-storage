#!/usr/bin/env python3
"""
Python function that inserts a new document in a collection
based on kwargs
"""
import pymongo


def insert_school(mongo_collection, **kwargs):
    """ Returns the new _id
    """
    mongo_collection.insert_one(kwargs)
    results = mongo_collection.find().sort(_id:-1)

    for r in results:
        return r["_id"]
